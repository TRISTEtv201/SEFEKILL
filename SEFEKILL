function speed3()
if f == off then
    gg.setVisible(false)
    gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
    gg.searchNumber("h 01 00 00 00 02 2B 07 3D", gg.TYPE_BYTE)
    t10 = gg.getResults(gg.getResultsCount())
    gg.setValues(t10)
    gg.editAll("h 01 00 00 00 02 2B 80 3D", gg.TYPE_BYTE)
    gg.clearResults()
    gg.toast("SPEED HACK TIME 50X ON")
    f = on
elseif f == on then
  gg.clearResults()  
    gg.setValues(t10)
    gg.editAll("h 01 00 00 00 02 2B 07 3D", gg.TYPE_BYTE)
    gg.toast("SPEED HACK TIME 50X OFF")
    gg.clearResults()
    gg.setVisible(false)
    f = off
end
end

